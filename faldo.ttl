# baseURI: http://biohackathon.org/faldo

@prefix :        <http://biohackathon.org/faldo#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix spin:    <http://spinrdf.org/spin#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

<http://biohackathon.org/faldo>
      rdf:type owl:Ontology ;
      spin:imports <http://topbraid.org/spin/owlrl-all> , <http://topbraid.org/spin/rdfsplus> , <http://topbraid.org/spin/spinowl> ;
      owl:versionInfo "Created with TopBraid Composer"^^xsd:string .

:BothStrandsPosition
      rdf:type owl:Class ;
      rdfs:comment "The both strands position mean that the region spans both strands instead of one."^^xsd:string ;
      rdfs:label "0"^^xsd:string ;
      rdfs:subClassOf :StrandedPosition ;
      owl:disjointWith :ForwardStrandPosition , :ReverseStrandPosition .

:ExactPosition
      rdf:type owl:Class ;
      rdfs:comment "Use when you exactly know the position."^^xsd:string ;
      rdfs:label "Exact position"^^xsd:string ;
      rdfs:subClassOf :Position ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :position
              ] ;
      owl:disjointWith :OneOfPosition , :InbetweenPosition , :InRangePosition .

:ForwardStrandPosition
      rdf:type owl:Class ;
      rdfs:comment "The position is on the forward (positive) strand."^^xsd:string ;
      rdfs:label "-"^^xsd:string ;
      rdfs:subClassOf :StrandedPosition ;
      owl:disjointWith :BothStrandsPosition , :ReverseStrandPosition .

:FuzzyPosition
      rdf:type owl:Class ;
      rdfs:comment "Use this class to indicate that you lack exact position data."^^xsd:string ;
      rdfs:subClassOf :Position .
=======

:InRangePosition
      rdf:type owl:Class ;
      rdfs:comment "Use when you have an idea of the range in which you can find the position but can not be sure."^^xsd:string ;
      rdfs:label "In the inclusive range from begin to end."^^xsd:string ;
      rdfs:subClassOf :FuzzyPosition ;
      rdfs:subClassOf
              [ rdf:type owl:Class ;
                owl:intersectionOf ([ rdf:type owl:Restriction ;
                            owl:onProperty :begin ;
                            owl:someValuesFrom :Position
                          ] [ rdf:type owl:Restriction ;
                            owl:onProperty :end ;
                            owl:someValuesFrom :Position
                          ])
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Class ;
                owl:intersectionOf ([ rdf:type owl:Restriction ;
                            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty :begin
                          ] [ rdf:type owl:Restriction ;
                            owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty :end
                          ])
              ] ;
      owl:disjointWith :OneOfPosition , :StrandedPosition , :ExactPosition , :InbetweenPosition .

:InbetweenPosition
      rdf:type owl:Class ;
      rdfs:comment "This denotes that a feature is in between two other positions that are both known exactly and next to eaxh other. An example is an restriction enzyme cutting site. The cut is after one nucleotide and before the next i.e. in between"^^xsd:string ;
      rdfs:label "In between positions."^^xsd:string ;
      rdfs:subClassOf :Position ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:hasValue :Position ;
                owl:onProperty :before
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Class ;
                owl:intersectionOf ([ rdf:type owl:Restriction ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onProperty :before
                          ] [ rdf:type owl:Class ;
                            owl:intersectionOf ([ rdf:type owl:Restriction ;
                                        owl:onProperty :before ;
                                        owl:someValuesFrom :ExactPosition
                                      ] [ rdf:type owl:Restriction ;
                                        owl:cardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onProperty :after
                                      ] [ rdf:type owl:Restriction ;
                                        owl:onProperty :after ;
                                        owl:someValuesFrom :ExactPosition
                                      ])
                          ])
              ] ;
      owl:disjointWith :OneOfPosition , :InRangePosition , :ExactPosition .

:OneOfPosition
      rdf:type owl:Class ;
      rdfs:comment "The position must be one of the positions listed by the position predicate."^^xsd:string ;
      rdfs:label "One of these positions"^^xsd:string ;
      rdfs:subClassOf :FuzzyPosition ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:onProperty :possiblePosition ;
                owl:someValuesFrom :Position
              ] ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:minCardinality "2"^^xsd:nonNegativeInteger ;
                owl:onProperty :possiblePosition
              ] ;
      owl:disjointWith :ExactPosition , :InbetweenPosition , :InRangePosition .

:Position
      rdf:type owl:Class ;
      rdfs:subClassOf owl:Thing ;
      rdfs:subClassOf
              [ rdf:type owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty :reference
              ] .

:PositionOnDNASequence
      rdf:type owl:Class ;
      rdfs:comment "This position is described in relation to a DNA sequence"^^xsd:string ;
      rdfs:subClassOf :StrandedPosition .

:PositionOnDoubleStrandedRNASequence
      rdf:type owl:Class ;
      rdfs:comment "This position is described in relation to a double stranded RNA sequence"^^xsd:string ;
      rdfs:subClassOf :StrandedPosition .

:PositionOnForwardSingleStrandRNASequence
      rdf:type owl:Class ;
      rdfs:comment "This position is described in relation to a forward RNA sequence"^^xsd:string ;
      rdfs:subClassOf :ForwardStrandPosition .

:PositionOnSingleStrandReverseRNASequence
      rdf:type owl:Class ;
      rdfs:comment "This position is described in relation to a (-) reverse RNA sequence"^^xsd:string ;
      rdfs:subClassOf :ReverseStrandPosition .

:Region
      rdf:type owl:Class ;
      rdfs:comment "A region describes an length of sequence with a start and end position that represents a feature on a Sequence. i.e. A gene"^^xsd:string ;
      rdfs:label "Region"^^xsd:string ;
      rdfs:subClassOf owl:Thing ;
      rdfs:subClassOf
              [ rdf:type owl:Class ;
                owl:intersectionOf ([ rdf:type owl:Class ;
                            owl:intersectionOf ([ rdf:type owl:Restriction ;
                                        owl:hasValue :Position ;
                                        owl:onProperty :begin
                                      ] [ rdf:type owl:Restriction ;
                                        owl:cardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onProperty :begin
                                      ])
                          ] [ rdf:type owl:Class ;
                            owl:intersectionOf ([ rdf:type owl:Restriction ;
                                        owl:hasValue :Position ;
                                        owl:onProperty :end
                                      ] [ rdf:type owl:Restriction ;
                                        owl:cardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onProperty :end
                                      ])
                          ])
              ] .

:ReverseStrandPosition
      rdf:type owl:Class ;
      rdfs:comment "The position is on the reverse (complement) strand of the sequence."^^xsd:string ;
      rdfs:label "+"^^xsd:string ;
      rdfs:subClassOf :StrandedPosition ;
      owl:disjointWith :ForwardStrandPosition , :BothStrandsPosition .

:StrandedPosition
      rdf:type owl:Class ;
      rdfs:comment "Part of the coordinate system is on which strand the feature can be found. If you do not yet know which stand the feature is on you should tag the position with just this class. If you know more you should use one of the subcalsses."^^xsd:string ;
      rdfs:label "Stranded Position"^^xsd:string ;
      rdfs:subClassOf :Position .

:after
      rdf:type owl:ObjectProperty ;
      rdfs:comment "This predicate is used when you want to describe a non inclusive range. Only used in the in between position to say it is after a nucleotide but before the next one."^^xsd:string ;
      rdfs:domain :InbetweenPosition ;
      rdfs:range :ExactPosition .

:before
      rdf:type owl:ObjectProperty ;
      rdfs:domain :InbetweenPosition ;
      rdfs:range :ExactPosition .

:begin
      rdf:type owl:ObjectProperty ;
      rdfs:range :Position .

:end  rdf:type owl:ObjectProperty ;
      rdfs:range :Position .

:location
      rdf:type owl:ObjectProperty .

:position
      rdf:type owl:DatatypeProperty ;
      rdfs:comment "A position on the first amino acid or nucleotide of a sequence has the value 1. i.e. Python style array indexing not java/C style."^^xsd:string , "The positionValue is the offset along the reference where this position is found. Thus the only the positionValue in combination with the reference determines where a Position is."^^xsd:string ;
      rdfs:domain :ExactPosition ;
      rdfs:range xsd:integer .

:possiblePosition
      rdf:type owl:ObjectProperty ;
      rdfs:domain :OneOfPosition ;
      rdfs:range :Position .

:reference
      rdf:type owl:ObjectProperty ;
      rdfs:comment "The reference is the resource that determines where the positionValue offsets into.  For example it points to a contig or a genome assembly."^^xsd:string .
